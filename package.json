{
  "name": "codeceptjs-testrail",
  "version": "1.12.0",
  "description": "CodeceptJS plugin for TestRail",
  "main": "index.js",
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KobeNguyenT/codeceptjs-testrail.git"
  },
  "keywords": [
    "codeceptJS",
    "codeceptjs",
    "testrail",
    "testrail-plugin"
  ],
  "author": "kobenguyent <kobenguyent@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KobeNguyenT/codeceptjs-testrail/issues"
  },
  "homepage": "https://github.com/KobeNguyenT/codeceptjs-testrail#readme",
  "dependencies": {
    "form-data": "3.0.0",
    "sprintf-js": "1.1.1",
    "testrail-api": "1.3.5"
  },
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test": "npm run test:unit && npm run test:esm && npm run acceptance_test",
    "test:unit": "mocha test/unit_test.js --timeout 6000",
    "test:esm": "mocha test/esm_test.mjs --timeout 6000",
    "test-server": "json-server test/db.json --routes test/routes.json &",
    "acceptance_test": "mocha test/acceptance_test.js --timeout 6000",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "axios": "1.8.2",
    "axios-mock-adapter": "1.19.0",
    "chai": "4.2.0",
    "chalk": "4.0.0",
    "codeceptjs": "3.5.8",
    "eslint": "8.2.0",
    "eslint-plugin-codeceptjs": "^1.3.0",
    "json-server": "0.17.0",
    "mocha": "10.2.0",
    "semantic-release": "21.0.1"
  },
  "files": [
    "lib/*",
    "index.js",
    "index.mjs"
  ]
}
